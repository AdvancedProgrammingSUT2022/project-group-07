<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="700.0" prefWidth="1100.0" styleClass="anchorPane" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.Client.View.controller.MainMenuController">
    <stylesheets>
        <URL value="@../css/mainPage.css"/>
    </stylesheets>
    <children>
        <Button fx:id="back" layoutX="290.0" layoutY="604.0" mnemonicParsing="false" onAction="#logout"
                prefHeight="14.0" prefWidth="520.0" text="Back">
            <font>
                <Font name="System Bold" size="23.0"/>
            </font>
        </Button>
        <Button fx:id="logout" layoutX="290.0" layoutY="533.0" mnemonicParsing="false" onAction="#logout"
                prefHeight="56.0" prefWidth="520.0" text="Logout">
            <font>
                <Font name="System Bold" size="26.0"/>
            </font>
        </Button>
        <Button fx:id="profileMenu" layoutX="290.0" layoutY="331.0" mnemonicParsing="false" onAction="#goToProfile"
                prefHeight="56.0" prefWidth="520.0" text="Profile Menu">
            <font>
                <Font name="System Bold" size="26.0"/>
            </font>
        </Button>
        <Button fx:id="gameMenu" layoutX="290.0" layoutY="264.0" mnemonicParsing="false" onAction="#goToGameMenu"
                prefHeight="56.0" prefWidth="520.0" text="Game Menu">
            <font>
                <Font name="System Bold" size="26.0"/>
            </font>
        </Button>
        <Button fx:id="scoreBoard" layoutX="290.0" layoutY="466.0" mnemonicParsing="false" onAction="#goToScoreBoard"
                prefHeight="56.0" prefWidth="520.0" text="ScoreBoard">
            <font>
                <Font name="System Bold" size="26.0"/>
            </font>
        </Button>
        <Button fx:id="chatMenu" layoutX="290.0" layoutY="399.0" mnemonicParsing="false" onAction="#goToChatMenu"
                prefHeight="13.0" prefWidth="520.0" text="Chat Menu">
            <font>
                <Font name="System Bold" size="26.0"/>
            </font>
        </Button>
    </children>
</AnchorPane>
