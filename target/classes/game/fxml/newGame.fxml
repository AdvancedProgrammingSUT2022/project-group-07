<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="700.0" prefWidth="1100.0" styleClass="anchorPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.View.controller.newGameController">
    <stylesheets>
        <URL value="@../css/newGamePage.css" />
    </stylesheets>
    <children>
        <Button fx:id="backButton" layoutX="37.0" layoutY="626.0" mnemonicParsing="false" onAction="#goBack" prefHeight="39.0" prefWidth="497.0" text="Back">
            <font>
                <Font name="System Bold" size="26.0" />
            </font>
        </Button>
        <Button fx:id="startButton" layoutX="549.0" layoutY="626.0" mnemonicParsing="false" onAction="#start" prefHeight="33.0" prefWidth="521.0" text="Start">
            <font>
                <Font name="System Bold" size="27.0" />
            </font>
        </Button>
        <ListView fx:id="allPlayers" layoutX="179.0" layoutY="83.0" onDragDropped="#removeE" prefHeight="462.0" prefWidth="221.0" />
        <ListView fx:id="invitedPlayers" layoutX="635.0" layoutY="83.0" prefHeight="462.0" prefWidth="235.0" />
        <Text layoutX="138.0" layoutY="59.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="font" text="Players List" textAlignment="CENTER" wrappingWidth="302.6708984375">
        </Text>
        <Text layoutX="588.0" layoutY="59.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="font" text="Players Invited" textAlignment="CENTER">
        </Text>
        <Button fx:id="addButton" layoutX="426.0" layoutY="314.0" mnemonicParsing="false" onAction="#addPlayer" prefHeight="39.0" prefWidth="181.0" text="add">
            <font>
                <Font size="25.0" />
            </font>
        </Button>
        <Button fx:id="removeButton" layoutX="426.0" layoutY="384.0" mnemonicParsing="false" onAction="#remove" prefHeight="21.0" prefWidth="181.0" text="remove">
            <font>
                <Font size="26.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
