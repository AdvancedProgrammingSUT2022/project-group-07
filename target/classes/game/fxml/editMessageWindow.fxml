<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane
        prefHeight="370.0" prefWidth="500.0"
        xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="game.View.controller.ChatControllers.EditMessageWindowController">
   <children>
      <Button fx:id="cancelButton" layoutX="200.0" layoutY="316.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="Cancel" />
      <Button fx:id="confirmButton" layoutX="100.0" layoutY="185.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="100.0" text="Confirm" />
      <TextField fx:id="messageTextField" layoutX="200.0" layoutY="135.0" prefHeight="100.0" prefWidth="200.0" />
      <Button fx:id="deleteButton" layoutX="100.0" layoutY="135.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="100.0" text="Delete" />
      <ChoiceBox fx:id="choiceBox" layoutX="150.0" layoutY="257.0" prefHeight="25.0" prefWidth="200.0" />
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="175.0" layoutY="40.0" prefHeight="60.0" prefWidth="150.0" text="Edit your message here" textAlignment="CENTER" />
      <Label fx:id="selectedLabel" layoutX="40.0" layoutY="53.0" text="Label" />
   </children>
</AnchorPane>
